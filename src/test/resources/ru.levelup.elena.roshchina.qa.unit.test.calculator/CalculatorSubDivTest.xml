<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Calculator Division and Substraction Test">
    <test name="Long Division TESTt">
        <parameter name="a" value="120000000"/>
        <parameter name="b" value="60000000"/>
        <parameter name="expected" value="2"/>
        <classes>
            <class name="ru.levelup.elena.roshchina.qa.unit.test.calculator.CalculatorDivTest">
                <methods>
                    <exclude name="doubleDivTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Double Division TEST">
        <parameter name="a" value="15.0"/>
        <parameter name="b" value="10.0"/>
        <parameter name="expected" value="1.5"/>
        <classes>
            <class name="ru.levelup.elena.roshchina.qa.unit.test.calculator.CalculatorDivTest">
                <methods>
                    <exclude name="longDivTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Long Substraction TESTt">
        <parameter name="a" value="120000000"/>
        <parameter name="b" value="60000000"/>
        <parameter name="expected" value="60000000"/>
        <classes>
            <class name="ru.levelup.elena.roshchina.qa.unit.test.calculator.CalculatorSubTest">
                <methods>
                    <exclude name="doubleSubTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Double Substraction TEST">
        <parameter name="a" value="15.0"/>
        <parameter name="b" value="10.0"/>
        <parameter name="expected" value="5.0"/>
        <classes>
            <class name="ru.levelup.elena.roshchina.qa.unit.test.calculator.CalculatorSubTest">
                <methods>
                    <exclude name="longSubTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>