        </classes>
    </test>
    <test name="Long Division TESTt">
        <parameter name="a" value="120000000"/>
        <parameter name="b" value="60000000"/>
        <parameter name="expected" value="2"/>
        <classes>
            <class name="ru.levelup.elena.roshchina.qa.unit.test.calculator.CalculatorDivTest">
                <methods>
                    <exclude name="doubleDivTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Double Division TEST">
        <parameter name="a" value="15.0"/>
